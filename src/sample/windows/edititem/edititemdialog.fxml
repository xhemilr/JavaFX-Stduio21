            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints fillHeight="false" minHeight="10.0" prefHeight="30.0" valignment="CENTER" />
            <RowConstraints />
            <RowConstraints />
        </rowConstraints>
<!--         <children>-->
            <Label text="Received Date:" GridPane.rowIndex="1" />
            <Label text="Contact:" GridPane.rowIndex="2" />
            <Label text="Phone Number" GridPane.rowIndex="3" />
            <Label text="Product Type" GridPane.rowIndex="4" />
            <Label text="Serial Number" GridPane.rowIndex="5" />
            <Label text="Product Description" GridPane.rowIndex="6" />
            <Label text="Service Description" GridPane.rowIndex="7" />
            <Label text="Price" GridPane.rowIndex="10" />
            <TextField fx:id="txtReceivedDate" disable="true" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="txtContact" promptText="Contact Name" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="txtPhoneNumber" promptText="Phone Number" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="txtProductType" promptText="ex. Laptop, Tablet, Phone, etc." GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="txtSerialNumber" promptText="Serial Number" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="txtProductDescription" promptText="Brand, Model, etc." GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <TextArea fx:id="txtComments" prefHeight="60.0" prefWidth="200.0" promptText="Comment for service request" GridPane.columnIndex="1" GridPane.rowIndex="7" GridPane.rowSpan="3" />
            <TextField fx:id="txtPrice" promptText="Price in MKD" GridPane.columnIndex="1" GridPane.rowIndex="10" />
            <Pane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="12">
<!--               <children>-->
                  <CheckBox fx:id="service" layoutY="2.0" mnemonicParsing="false" text="Serviced" />
                  <CheckBox fx:id="payment" layoutX="112.0" layoutY="2.0" mnemonicParsing="false" text="Payment Done" />
<!--               </children>-->
            </Pane>
            <Button fx:id="btnPrint" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="59.0" text="Print" GridPane.rowIndex="12">
               <GridPane.margin>
