<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<DialogPane maxHeight="220.0" maxWidth="350.0" minHeight="144.0" minWidth="350.0" prefHeight="156.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.windows.settings.SettingsController">
   <content>
      <GridPane alignment="TOP_CENTER" hgap="15.0" maxHeight="237.0" maxWidth="376.0" minHeight="118.0" minWidth="350.0" prefHeight="118.0" prefWidth="350.0" vgap="30.0">
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints />
        </rowConstraints>
<!--         <children>-->
            <Label text="Server IP Address:" />
            <Label text="Port Number:" GridPane.rowIndex="1" />
            <TextField fx:id="txtIpAddress" promptText="ex: 172.10.10.200" GridPane.columnIndex="1" />
            <TextField fx:id="txtPortNumber" promptText="ex: 8090" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="btnTestConnection" mnemonicParsing="false" text="Test Connection" GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <HBox prefHeight="182.0" prefWidth="196.0" spacing="30.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
<!--               <children>-->
                  <Button fx:id="btnSave" mnemonicParsing="false" prefHeight="25.0" prefWidth="72.0" text="Save" />
                  <Button fx:id="btnClose" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Close" />
<!--               </children>-->
            </HBox>
<!--         </children>-->
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
      </GridPane>
   </content>
   <header>
      <Label alignment="CENTER" text="Server Connection Settings" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </header>
</DialogPane>
